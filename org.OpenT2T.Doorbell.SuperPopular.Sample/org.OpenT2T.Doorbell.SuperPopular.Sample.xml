<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://standards.freedesktop.org/dbus/introspect-1.0.dtd"[]>
<node>
  <interface name="org.OpenT2T.Doorbell.SuperPopular.Sample">
    
   <!-- Determines if the doorbell is pressed -->
    <method name="isButtonPressed">
      <arg name="pressedEvent" type="b" direction="out" />
    </method>
    <!-- Gets the doorbell battery level -->
    <method name="getBatteryLevel" />
        <arg name="batteryLevel" type="d" direction="out" />
    </method>
    <!-- Called when there is an error during usage of this translator -->
    <signal name="error">
        <arg name="type" type="s" direction="out"/>
        <arg name="message" type="s" direction="out"/>
    </signal>
  </interface>
</node>