<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://standards.freedesktop.org/dbus/introspect-1.0.dtd"[]>
<node>
  <interface name="org.opent2t.sample.hub.superpopular">
    <description>OpenT2T SuperPopular Hub interface</description>

    <property name="name" type="s" access="read">
     <description>ReadOnly, Gets the name of the hub.</description>
    </property>
    
    <property name="devices" type="a(s)" access="read">
     <description>ReadOnly, Gets the current set of devices associated with the hub.</description>
    </property>
    
  </interface>
</node>